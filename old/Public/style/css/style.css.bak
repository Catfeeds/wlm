#new-header { clear: both; }
#new-nav li { float: left; width: 97px; height: 49px; padding-left: 25px; }
#new-nav li a { position:relative;outline: none; line-height: 49px; font-size: 16px; text-decoration: none; width: 100%; display: inline-block;}
#new-nav li a span{margin-left: 20px;}
.new-nav { display: inline-block; width: 16px; height: 16px; position: relative; top: 2px; margin-right: 4px;}
.new-nav-db { background-position: left top; }
.new-nav-today { background-position: -24px top; }
.new-nav-blog { background-position: -48px top; }
.new-nav-activity { background-position:  -72px  top; }
.new-nav-sh { background-position:  -96px top; }
.new-nav-archive { background-position:  -120px top; }
