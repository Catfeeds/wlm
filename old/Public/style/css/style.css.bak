	list-style: none;
	margin: 0 !important;
	padding: 0;
	padding: 2px 0px;
	position: absolute;
	top: 23px;
	width: 80px;
	z-index: 16;
	/* CSS3 */
	-webkit-border-bottom-right-radius: 3px;
	-webkit-border-bottom-left-radius: 3px;
	-moz-border-radius-bottomright: 3px;
	-moz-border-radius-bottomleft: 3px;
	border-bottom-right-radius: 3px;
	border-bottom-left-radius: 3px;
  }
  .frep-ui-select-wrap:focus,
  .frep-ui-select-wrap:active {
	outline: 0;
  }
	.frep-ui-select-list li {
	  cursor: pointer;
	  display: block;
	  margin: 0;
	  padding: 0;
	}
	  .frep-ui-select-list li span {
		display: block;
		border-bottom: 1px solid #f6f6f6;
		padding: 6px 5px 6px 5px;
		margin: 0 5px;
	  }
