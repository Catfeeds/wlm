/* today */
.day .media {
		margin-top: 0;
		padding-top: 15px;
		padding-bottom: 8px;
}

.day .media:hover {
		background: #eee;
		cursor:pointer;
		border-radius: 3px;

}

.day .media-object {
	background: none repeat scroll 0 0 #eee;
	padding: 4px 10px;
	text-decoration: none;
	position: relative;
	top: -4px;
}

.day p {
	color: #ada8a2;
	font-size: 12px;
}

.day .media-object strong {
	display: block;
	font-weight: 400;
	margin-top: 4px;
	text-align: center;
}

.day .media-object span {
	display: block;
	text-align: center;
}

