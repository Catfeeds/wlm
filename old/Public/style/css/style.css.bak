/*QR code*/
.qrcode{position:fixed;right:30px;bottom:135px;width:50px;height:50px;}
.qrcode a{width:50px;height:50px;display:block;background: #666;background: url(img/qrcode.jpg) no-repeat;}
.qrcode a img{max-width: 160px; width:160px;height:160px;position:absolute;background:#fff;border:1px solid #cacaca;display:none;}
.qrcode a:hover{}
.qrcode a:hover img{display:block;left:-163px;top:-112px;}

/*dialog box*/
#dialog_body {
  width: 980px;
  margin: 0 auto;
  /*height: 400px;*/
  background: #fff;
  filter:alpha(opacity=97);       /* IE */
  -moz-opacity:.97;              /* 老版Mozilla */
  -khtml-opacity:.97;              /* 老版Safari */
  opacity: .97;           /* 支持opacity的浏览器*/
  position: fixed;
  top: -400px;
  box-shadow:0 5px 8px rgba(0, 0, 0, 0.3);
  -moz-box-shadow:0 5px 8px rgba(0, 0, 0, 0.3);
  -webkit-box-shadow: 0 5px 8px rgba(0, 0, 0, 0.3);
}
.ml50 {margin-left: 50px;}
.opacity80 {filter:alpha(opacity=80);-moz-opacity:.8;-khtml-opacity:.8;opacity: .8;}
.opacity90 {filter:alpha(opacity=90);-moz-opacity:.9;-khtml-opacity:.9;opacity: .9;}
.loading {padding-left: 27px; background: url(img/loading.gif) 7px center no-repeat #314751;}
.loading:hover {background-position: 7px center;}

/*dialog*/
#dialog {text-align: center;position: fixed;top: -200px;z-index: 99999;background: #fff;}
#dialog .text {font-size: 20px;background: url(img/ok.png) left center no-repeat;background-size:40px;height: 50px;display: inline-block;padding-left: 60px;margin: 20px 150px;line-height: 50px;}
#dialog .title {line-height: 30px;text-align: center;font: bold 14px arial;padding: 10px;background: #314751;color: #fff;}
#dialog .exit {background: url(img/close.png) 50% 50% no-repeat;display: inline-block;width: 25px;height: 25px;background-size: 12px;cursor: pointer;margin-top: -5px;margin-right: -5px;}
#dialog .body {margin:40px 80px;}

#dialog .form-horizontal .control-label {width: 60px;}
#dialog .form-horizontal .controls {margin-left: 80px; text-align: left;}
#dialog .form-horizontal textarea {width:330px;}

#mask {z-index: 9999;width: 100%;height: 100%;background: #000;position: fixed;top: 0;left: 0;opacity: .4;}

